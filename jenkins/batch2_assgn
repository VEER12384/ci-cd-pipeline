pipeline 
{
  agent none
        stages{
        stage('deploy')
        {
        steps
        {
        sh "echo this is the deploy stage"
        }
        }
        stage ('test')
        {
        
               stage('testing1')
        {
        when {
                branch 'main'
            }
        steps{
       sh "echo this is a testing 1 stage"
}
}
stage ('testing2')
{
 when {
                branch 'main'
            }
steps
{
    sh "echo this is testing 2 stage"
}
}
}
}
}
